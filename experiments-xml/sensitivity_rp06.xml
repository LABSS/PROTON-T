<experiments>
   <!--                               1296 runs -->
  <experiment name="sensitivity_rp06" repetitions="1" runMetricsEveryStep="true">
    <setup>setup</setup>
    <go>go</go>
    <final>show (word "Time elapsed:" timer)
    </final>   
    <!--                                assuming 1 year = 8640 steps, three = 25920  -->
    <timeLimit steps="200"/>
