    <metric>t5 "mean" "Collective relative deprivation" 2</metric>  
    <metric>t5 "mean" "Collective relative deprivation" 3</metric>
    <metric>t5 "mean" "Collective relative deprivation" 4</metric> 
    <metric>t5 "median" "Non integration" 0</metric>
    <metric>t5 "median" "Non integration" 1</metric>
    <metric>t5 "median" "Non integration" 2</metric>
    <metric>t5 "median" "Non integration" 3</metric>
    <metric>t5 "median" "Non integration" 4</metric>
    <metric>t5 "median" "Institutional distrust" 0</metric>
    <metric>t5 "median" "Institutional distrust" 1</metric>
