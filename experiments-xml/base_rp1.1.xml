<experiments>
  <experiment name="base_rp1.1" repetitions="10" runMetricsEveryStep="true">
    <setup>
        setup
        export-risk
        export-opinions
    </setup>
    <go>go</go>
    <final>
        export-risk
        export-opinions
        show (word "Time elapsed:" timer)
    </final>   
    <!--                                assuming 6 months = 3060 steps with 17-hours days  -->
    <timeLimit steps="50"/>
    <!--                               Metrics for the wizard   -->
    <!-- t1 recruited citizens -->
    <metric>t1</metric>
    <!-- t2 citizen at risk (over threshold) -->
    <metric>t2</metric>
    <!-- t3 risk of citizens -->
    <metric>t3 "mean" 0</metric>
    <metric>t3 "median" 0</metric>
    <metric>t3 "standard-deviation" 0</metric>
    <metric>t3 "mean" 1</metric>
    <metric>t3 "median" 1</metric>
    <metric>t3 "standard-deviation" 1</metric>
    <metric>t3 "mean" 2</metric>
    <metric>t3 "median" 2</metric>
    <metric>t3 "standard-deviation" 2</metric>
    <metric>t3 "mean" 3</metric>
    <metric>t3 "median" 3</metric>
    <metric>t3 "standard-deviation" 3</metric>
    <metric>t3 "mean" 4</metric>
    <metric>t3 "median" 4</metric>
    <metric>t3 "standard-deviation" 4</metric>
    <metric>t3.bins</metric>
    <metric>t4 "mean" 0</metric>
    <metric>t4 "median" 0</metric>
    <metric>t4 "standard-deviation" 0</metric>
    <metric>t4 "mean" 1</metric>
    <metric>t4 "median" 1</metric>
