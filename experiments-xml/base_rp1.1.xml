    <metric>t5.bins "Non integration"</metric>
    <metric>t5.bins "Institutional distrust"</metric>
    <metric>t5.bins "Collective relative deprivation"</metric>
    <metric>t5 "mean" "Non integration" 0</metric>
    <metric>t5 "mean" "Non integration" 1</metric>
    <metric>t5 "mean" "Non integration" 2</metric>
    <metric>t5 "mean" "Non integration" 3</metric>
    <metric>t5 "mean" "Non integration" 4</metric>
    <metric>t5 "mean" "Institutional distrust" 0</metric>
    <metric>t5 "mean" "Institutional distrust" 1</metric>
    <metric>t5 "mean" "Institutional distrust" 2</metric>
    <metric>t5 "mean" "Institutional distrust" 3</metric>
    <metric>t5 "mean" "Institutional distrust" 4</metric> 
    <metric>t5 "mean" "Collective relative deprivation" 0</metric>
