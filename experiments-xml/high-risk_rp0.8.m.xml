<experiments>
  <experiment name="high-risk_rp0.8" repetitions="3" runMetricsEveryStep="true">
    <setup>setup</setup>
  <go>go</go>
  <final>show (word "Time elapsed:" timer)
    </final>
  <timeLimit steps="6120" />
  <metric>t1</metric>
  <metric>t2</metric>
  <metric>t3 "mean" 0</metric>
  <metric>t4 "mean" 0</metric>
  <metric>t5 "mean" "Non integration" 0</metric>
  <metric>t5 "mean" "Institutional distrust" 0</metric>
  <metric>t5 "mean" "Collective relative deprivation" 0</metric>
  <metric>t5 "median" "Non integration" 0</metric>
  <metric>t5 "median" "Institutional distrust" 0</metric>
  <metric>t5 "median" "Collective relative deprivation" 0</metric>
  <metric>t5 "standard-deviation" "Non integration" 0</metric>
  <metric>t5 "standard-deviation" "Institutional distrust" 0</metric>
  <metric>t5 "standard-deviation" "Collective relative deprivation" 0</metric>
  <metric>t6  "residence" 0</metric>
  <metric>t6  "coffee" 0</metric>
  <metric>t6  "public space" 0</metric>
  <metric>t6  "propaganda place" 0</metric>
  <metric>t6  "community center" 0</metric>
  <metric>soc-counter</metric>
  <metric>soc-online-counter</metric>
  <metric>rec-counter</metric>
  <metric>min [ hours-to-recruit ] of citizens</metric>
  <metric>mean [ hours-to-recruit ] of citizens</metric>
  <metric>count links</metric>
  <metric>fail-activity-counter</metric>
  <metric>police-actions-counter</metric>
  <enumeratedValueSet variable="high-risk-employed">
      <value value="75" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="total-citizens">
      <value value="40000" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="alpha">
      <value value="0.1" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="population-employed-%">
      <value value="10" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="activity-radius">
      <value value="5" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="activity-debug?">
      <value value="false" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="work-socialization-probability">
      <value value="0.1" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="number-workers-per-community-center">
      <value value="1" />
